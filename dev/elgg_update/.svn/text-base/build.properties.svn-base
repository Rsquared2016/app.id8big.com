# General properties
	#Name of the elgg zip file
	general.elgg_version_file_name=elgg1.7.4
	#Support tar.gz too!! :P
	general.elgg_version_file_ext=zip
	#Conditional, if true the mod folder will be excluded from the update.
	general.elgg_mod_exclude=false

# Svn properties
	#Change the project type.
	svn.repository_proj_type=keetup
	#Change the project name.
	svn.repository_proj_name=elggbase
	#Set the current elgg version
	svn.branch_version=173


# PROPIEDADES ESTATICAS, NO DEBERIAN CAMBIARSE.
# General properties
	#Folder that container the elgg zip file, should be current folder
	#NO CAMBIAR.
	general.elgg_version_folder=./
	
	#This is the name of the file, where the new elgg version will be unzipped, should not be modified
	#NO CAMBIAR.
	general.elgg_tmp_folder=./tmp/

# Update properties
	#Base dir, relative to this file.
	#NO CAMBIAR.
	update.project_base_dir=../../

# Svn properties
	#Change the name of the project, check if in sites
	#NO CAMBIAR
	svn.repository_url=svn://keetup.mine.nu
	svn.repository_trunk=trunk
	svn.repository_branch=branches
	
	#Default message.
	svn.branch_message=Se creo el branch de la version de elgg 

# Email properties
	#NO usado.
	mail.recipient=dgallardo@keetup.com
